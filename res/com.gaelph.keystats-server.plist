<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>com.gaelph.keystats-server</string>
		<key>Program</key>
		<string>{{ KEYSTATS_HOME }}/bin/keystats-server</string>
		<key>WorkingDirectory</key>
		<string>{{ KEYSTATS_HOME }}</string>
		<key>EnvironmentVariables</key>
		<dict>
			<key>HOME</key>
			<string>{{ HOME }}</string>
			<key>PORT</key>
		  <string>12000</string>
		</dict>
		<key>StandardOutPath</key>
		<string>{{ HOME }}/.cache/keystats/server-out.log</string>
		<key>StandardErrorPath</key>
		<string>{{ HOME }}/.cache/keystats/server-error.log</string>
		<key>ProcessType</key>
		<string>Background</string>
		<key>RunAtLoad</key>
		<true/>
		<key>KeepAlive</key>
		<true/>
	</dict>
</plist>

