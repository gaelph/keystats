{
  "name": "keystats",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "main": "build/main.cjs",
  "bin": {
    "keystats": "./bin/keystats"
  },
  "scripts": {
    "start": "nodemon",
    "bundle": "node esbuild.js",
    "clean": "rm -rf ./build",
    "build": "npm run clean && npm run bundle && pkg -t node18-macos-arm64 --out-path ./bin ./build/keystats.cjs && pkg -t node18-macos-arm64 --out-path ./bin ./build/keystats-server.cjs",
    "start:server": "cd src/client && npm run build && cd ../.. && nodemon --config nodemon-server.json",
    "start:client": "cd src/client && npm run start"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node-hid": "^1.3.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "knex": "^2.5.1",
    "loglevel": "^1.8.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "node-hid": "^2.1.2",
    "node-usb-detection": "^1.0.2",
    "sqlite3": "^5.1.6",
    "usb-detection": "^4.14.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "esbuild": "^0.19.4",
    "eslint": "^8.50.0",
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1",
    "typescript": "^5.2.2"
  },
  "pkg": {}
}
